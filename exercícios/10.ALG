algoritmo "10"
var
 largura,altura,areap,litrot:real
inicio
 escreva("Insira a largura da parede: ")
 leia(largura)
 escreva("Insira a altura da parede: ")
 leia(altura)
 areap<-largura*altura
 litrot<-areap/2
 escreva("A area a ser pintada e ",areap," e sera necessario ",litrot," litros de tinta.")
fimalgoritmo